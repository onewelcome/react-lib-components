{
  "name": "@onewelcome/react-lib-components",
  "version": "0.0.0-dev",
  "license": "Apache-2.0",
  "author": "OneWelcome B.V.",
  "main": "dist/index.js",
  "module": "dist/react-lib-components.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/onewelcome/react-lib-components"
  },
  "scripts": {
    "analyze": "size-limit --why",
    "build": "dts build",
    "build-storybook": "build-storybook",
    "dev": "npm-run-all -p start test:watch storybook",
    "lint": "eslint '**/*.{ts,tsx}'",
    "prepare": "husky install && dts build",
    "size": "size-limit",
    "start": "dts watch",
    "prettier": "prettier --write --ignore-path .gitignore \"**/*.+(ts|tsx|scss|mdx)\"",
    "storybook": "start-storybook -p 6006",
    "test": "dts test --passWithNoTests",
    "test:watch": "dts test --passWithNoTests --watch"
  },
  "jest": {
    "testEnvironment": "jsdom"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "engines": {
    "node": ">=16"
  },
  "size-limit": [
    {
      "path": "dist/react-lib-components.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/react-lib-components.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@mdx-js/react": "^1.6.22",
    "@onewelcome/eslint-config-shared-codestyle": "^9.0.3",
    "@size-limit/preset-small-lib": "^7.0.8",
    "@storybook/addon-a11y": "^6.5.12",
    "@storybook/addon-docs": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/addons": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.12",
    "@storybook/manager-webpack5": "^6.5.12",
    "@storybook/preset-scss": "^1.0.3",
    "@storybook/react": "^6.5.12",
    "@storybook/theming": "^6.5.12",
    "@testing-library/dom": "^8.18.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.5",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@tsconfig/create-react-app": "^1.0.2",
    "@tsconfig/recommended": "^1.0.1",
    "@types/color-convert": "^2.0.0",
    "@types/mdx": "^2.0.2",
    "@types/react": "^17.0.50",
    "@types/react-dom": "^17.0.17",
    "@types/react-router": "^5.1.19",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "babel-loader": "^8.2.5",
    "dts-cli": "^1.6.0",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^26.9.0",
    "eslint-plugin-license-header": "^0.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest-junit": "^13.2.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "react-router": "^6.4.2",
    "react-router-dom": "^6.4.2",
    "rollup-plugin-cleanup": "^3.2.1",
